<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lixindi.gradproject.dao.CandidateMapper">
    <resultMap id="BaseResultMap" type="com.lixindi.gradproject.vo.CandidateInfo">
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result column="candidateNum" property="candidatenum" jdbcType="VARCHAR"/>
        <result column="name" property="name" jdbcType="VARCHAR"/>
        <result column="age" property="age" jdbcType="INTEGER"/>
        <result column="major" property="major" jdbcType="VARCHAR"/>
        <result column="company" property="company" jdbcType="VARCHAR"/>
        <result column="department" property="department" jdbcType="VARCHAR"/>
        <result column="group" property="group" jdbcType="VARCHAR"/>
    </resultMap>

    <insert id="insertCandidate">
        INSERT INTO
        candidate(candidateNum,name,age,major,company,department)
        VALUES
        <foreach collection="list" item="item" separator=",">
            (#{item.candidateNum}, #{item.name}, #{item.age}, #{item.major}, #{item.company}, #{item.department})
        </foreach>
    </insert>
</mapper>